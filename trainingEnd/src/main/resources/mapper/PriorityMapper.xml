        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.training.mapper.PriorityMapper">

    <resultMap id="BaseResultMap" type="com.training.pojo.Priority">
            <id property="id" column="id" jdbcType="INTEGER"/>
